<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="seasonsplayingVM" type="com.mob.app.modules.seasonsplaying.data.viewmodel.SeasonsplayingVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSeasonsPlaying"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyleyellow_100">
        <LinearLayout android:id="@+id/linearColumneasterbunnyone"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_35pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRoweasterbunnyone"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout
                        android:id="@+id/frameStackeasterbunnyone"
                        android:layout_height="@dimen/_81pxv"
                        android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_84pxh">
                        <ImageView android:id="@+id/imageEasterbunnyOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_81pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_79pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_easterbunny1}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_easterbunny1"/>
                        <View
                            android:background="@drawable/rectangle_bg_blue_100_radius_5"
                            android:id="@+id/viewEllipse781"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_35pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_35pxv" android:layout_width="@dimen/_10pxh"/>
                    </FrameLayout>
                    <View
                        android:background="@drawable/rectangle_bg_blue_100_radius_7"
                        android:id="@+id/viewEllipse780"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_41pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_37pxv" android:layout_width="@dimen/_14pxh"/>
                    <FrameLayout android:id="@+id/frameStackellipse779"
                        android:layout_height="@dimen/_68pxv"
                        android:layout_marginBottom="@dimen/_24pxv"
                        android:layout_marginStart="@dimen/_3pxh" android:layout_width="@dimen/_208pxh">
                        <View
                            android:background="@drawable/rectangle_bg_blue_100_radius_104"
                            android:id="@+id/viewEllipse779"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_68pxv" android:layout_width="@dimen/_208pxh"/>
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtAadakigrse"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:text="@{seasonsplayingVM.seasonsplayingModel.txtAadakigrse}"
                            style="@style/txtABeeZeeitalic20" tools:text="@string/msg_a_a_daki_g_rse"/>
                    </FrameLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowicecreamone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcecreamOne"
                    android:layout_height="@dimen/_152pxh"
                    android:layout_marginTop="@dimen/_136pxv"
                    android:layout_width="@dimen/_142pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_icecream1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icecream1"/>
                <ImageView android:id="@+id/imageVacationsOne"
                    android:layout_height="@dimen/_159pxh"
                    android:layout_marginBottom="@dimen/_129pxv"
                    android:layout_width="@dimen/_154pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_vacations1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vacations1"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowellipse782"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_55pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackellipse782"
                    android:layout_height="@dimen/_71pxv"
                    android:layout_marginBottom="@dimen/_24pxv" android:layout_width="@dimen/_242pxh">
                    <View
                        android:background="@drawable/rectangle_bg_blue_100_radius_121"
                        android:id="@+id/viewEllipse782"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_71pxv" android:layout_width="@dimen/_242pxh"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtAadakikutuy"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_50pxh"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{seasonsplayingVM.seasonsplayingModel.txtAadakikutuy}"
                        style="@style/txtABeeZeeitalic19" tools:text="@string/msg_a_a_daki_kutuy"/>
                </FrameLayout>
                <View
                    android:background="@drawable/rectangle_bg_blue_100_radius_7"
                    android:id="@+id/viewEllipse783"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_41pxv"
                    android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_14pxh"/>
                <FrameLayout android:id="@+id/frameStackeasterbunnytwo"
                    android:layout_height="@dimen/_81pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_84pxh">
                    <ImageView android:id="@+id/imageEasterbunnyTwo"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_81pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_79pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_easterbunny2}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_easterbunny2"/>
                    <View
                        android:background="@drawable/rectangle_bg_blue_100_radius_5"
                        android:id="@+id/viewEllipse784"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_35pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_35pxv" android:layout_width="@dimen/_10pxh"/>
                </FrameLayout>
            </LinearLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:hint="@string/lbl_buraya_yaz"
                android:id="@+id/textInputLayMetinyazlacak"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:textColorHint="@color/black_900" style="@style/txtSolid">
                <com.google.android.material.textfield.TextInputEditText
                    android:ellipsize="end"
                    android:id="@+id/etMetinyazlacak"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@={seasonsplayingVM.seasonsplayingModel.etMetinyazlacakValue}"
                    android:textColor="@color/black_900" android:textSize="@dimen/_8pxh"/>
            </com.google.android.material.textfield.TextInputLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowkontroledelim"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_61pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_blue_100cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtKontroledelim"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_95pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.95"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_31pxh"
                    android:singleLine="true"
                    android:text="@{seasonsplayingVM.seasonsplayingModel.txtKontroledelim}"
                    style="@style/txtAksaraBaliGalangregular18" tools:text="@string/lbl_kontrol_edelim"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_103pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_7pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
